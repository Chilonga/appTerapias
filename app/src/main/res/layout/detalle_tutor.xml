<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="22dp"
        android:drawableLeft="@drawable/nombre"
        android:drawablePadding="10dp"
        android:text="Tutor:"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvNombreTDe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView18"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="50dp"
        android:layout_toEndOf="@+id/textView18"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tvcedulat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNombreTDe"
        android:layout_below="@+id/textView18"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/tvcedulat"
        android:drawableLeft="@drawable/cedula"
        android:drawablePadding="10dp"
        android:text="Cedula:"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvsexot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNombreTDe"
        android:layout_below="@+id/tvcedulat"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/tvsexot"
        android:drawableLeft="@drawable/sexo"
        android:drawablePadding="10dp"
        android:text="Sexo:"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvdirecct"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNombreTDe"
        android:layout_below="@+id/tvsexot"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp"
        android:text="Correo"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/tvdirecct"
        android:drawableLeft="@drawable/direccion"
        android:drawablePadding="10dp"
        android:text="Direccion:"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvcorreot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNombreTDe"
        android:layout_below="@+id/tvdirecct"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/tvcorreot"
        android:drawableLeft="@drawable/email"
        android:drawablePadding="10dp"
        android:text="Correo:"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvtelefonot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvNombreTDe"
        android:layout_below="@+id/tvcorreot"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/tvtelefonot"
        android:drawableLeft="@drawable/telefono"
        android:drawablePadding="10dp"
        android:text="Telefono:"
        android:textSize="16sp"
        android:textStyle="bold" />

</RelativeLayout>